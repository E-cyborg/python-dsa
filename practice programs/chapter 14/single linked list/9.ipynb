   "source": [
    "def insertion_sort(head,data):\n",
    "    if head == None:\n",
    "        return Node(data)\n",
    "    tmp =head\n",
    "    if data<tmp.data:\n",
    "        tmp= Node(data)\n",
    "        tmp.next(head)\n",
    "        return tmp\n",
    "    while tmp <data:\n",
    "        tmp=tmp.next\n",
    "    tmp= Node(data)\n",
    "    return head\n"
   ]
  }
