   "source": [
    "def delete_sp(head,pos):\n",
    "    if head is None:\n",
    "        return None\n",
    "    tmp=head\n",
    "    c=0\n",
    "    while tmp.next is not None and c<pos-1:\n",
    "        c+=1\n",
    "        tmp=tmp.next\n",
    "    tmp.next=tmp.next.next\n",
    "    return head\n"
   ]
  }
